   "source": [
    "def addDigits(num):\n",
    "    num = str(num)\n",
    "    if len(num) == 1:\n",
    "        return int(num)\n",
    "\n",
    "    sum = 0\n",
    "    for i in num:\n",
    "        sum += int(i)\n",
    "\n",
    "    return addDigits(sum)"
   ]
